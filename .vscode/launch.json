    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/stable_txt2img.py",
            "console": "integratedTerminal",
            "args": [
                "--ddim_eta", "0.0",
                "--n_samples", "8",
                "--n_iter", "1",
                "--scale", "10.0",
                "--ddim_steps", "50",
                "--ckpt", "./checkpoints/sd-v1-4-full-ema.ckpt",
                "--prompt", "red tullips in impressionism style"
            ]
        }
    ]
}